{% extends 'CwdAdminPortoBundle:Menu:menu.html.twig' %}

{% block list %}
    {% if item.hasChildren and options.depth is not sameas(0) and item.displayChildren %}
        {% import _self as knp_menu %}
        <ul class="nav nav-main">
            {{ block('children') }}
        </ul>
    {% endif %}
{% endblock %}